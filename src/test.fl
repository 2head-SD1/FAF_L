struct Favorite(color:String, number:Int);
struct Student(name:String, age:Int, favorite:<Favorite>);

setq olegsFavorite:<Favorite> Favorite("Orange", 77);
setq student:<Student> Student("Oleg", 20, olegsFavorite);

setq lol:Tuple tuple(1, 2, student);
((tuple-get(lol, 2)).favorite).color;

define firstElement (array : [Int]) -> Int {
    first(array)
};

setq myArray:[Int] Int[3]{4, 5, 6};
call firstElement(myArray);